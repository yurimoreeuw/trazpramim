<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/product_search_star_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/vendor_star_hollow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="#00FFFFFF" />
    <ImageView
        android:id="@+id/product_search_star_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/vendor_star_hollow"
        app:layout_constraintStart_toEndOf="@id/product_search_star_1"
        app:layout_constraintTop_toTopOf="parent"
        android:background="#00FFFFFF" />
    <ImageView
        android:id="@+id/product_search_star_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/vendor_star_hollow"
        app:layout_constraintStart_toEndOf="@id/product_search_star_2"
        app:layout_constraintTop_toTopOf="parent"
        android:background="#00FFFFFF"/>
    <ImageView
        android:id="@+id/product_search_star_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/vendor_star_hollow"
        app:layout_constraintStart_toEndOf="@id/product_search_star_3"
        app:layout_constraintTop_toTopOf="parent"
        android:background="#00FFFFFF" />
    <ImageView
        android:id="@+id/product_search_star_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/vendor_star_hollow"
        app:layout_constraintStart_toEndOf="@id/product_search_star_4"
        app:layout_constraintTop_toTopOf="parent"
        android:background="#00FFFFFF" />

</androidx.constraintlayout.widget.ConstraintLayout>